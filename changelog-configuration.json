{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["feature", "enhancement", "feat"]
    },
    {
      "title": "## 🐛 Bug Fixes",
      "labels": ["fix", "bugfix", "bug"]
    },
    {
      "title": "## 🔧 Maintenance",
      "labels": ["chore", "refactor", "perf", "ci", "build"]
    },
    {
      "title": "## 📚 Documentation",
      "labels": ["documentation", "docs"]
    },
    {
      "title": "## 🧪 Tests",
      "labels": ["test", "tests"]
    },
    {
      "title": "## 🔄 Dependencies",
      "labels": ["dependencies", "deps"]
    }
  ],
  "ignore_labels": [
    "ignore",
    "skip-changelog"
  ],
  "sort": {
    "order": "ASC",
    "on_property": "mergedAt"
  },
  "template": "${{CHANGELOG}}\n\n## 📦 Artifacts\n\nYou can find the crate on [crates.io](https://crates.io/crates/darwin-metrics/${{VERSION}}).\n\n## 🔍 Diff\n\n${{RELEASE_DIFF}}",
  "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
  "empty_template": "No changes in this release.",
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "max_back_track_time_days": 365,
  "exclude_merge_branches": [
    "Owner/main"
  ],
  "tag_resolver": {
    "method": "semver",
    "filter": {
      "pattern": "v(.+)",
      "flags": "g"
    }
  },
  "label_extractor": [
    {
      "pattern": "^(feat|feature|enhancement)(\\([\\w\\s-]+\\))?:\\s.+",
      "target": "$1",
      "flags": "gi"
    },
    {
      "pattern": "^fix(\\([\\w\\s-]+\\))?:\\s.+",
      "target": "fix",
      "flags": "gi"
    },
    {
      "pattern": "^(doc|docs)(\\([\\w\\s-]+\\))?:\\s.+",
      "target": "documentation",
      "flags": "gi"
    },
    {
      "pattern": "^(test|tests)(\\([\\w\\s-]+\\))?:\\s.+",
      "target": "test",
      "flags": "gi"
    },
    {
      "pattern": "^(chore|build|ci)(\\([\\w\\s-]+\\))?:\\s.+",
      "target": "chore",
      "flags": "gi"
    },
    {
      "pattern": "^(refactor|perf)(\\([\\w\\s-]+\\))?:\\s.+",
      "target": "refactor",
      "flags": "gi"
    }
  ],
  "transformers": [
    {
      "pattern": "(\\w+)\\((\\w+)\\):\\s*(.*)",
      "target": "[$2] $3"
    }
  ],
  "trim_values": true,
  "base_branches": [
    "main"
  ]
}